{
    "name": "bogdanghervan/irradiate",
    "description": "Database wizardry on top of Laravel excellence.",
    "support": {
        "issues": "https://github.com/bogdanghervan/irradiate/issues",
        "source": "https://github.com/bogdanghervan/irradiate"
    },
    "keywords": ["laravel", "irradiate", "database", "sql", "orm"],
    "license": "MIT",
    "authors": [
        {
            "name": "Bogdan Ghervan",
            "email": "bogdan.ghervan@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "illuminate/database": "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "Irradiate\\": "src/Irradiate"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Irradiate\\Database\\DatabaseServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
